{
  "swagger": "2.0",
  "info": {
    "version": "1.0.0",
    "title": "Google reCAPTCHA Verify"
  },
  "host": "www.google.com",
  "basePath": "/recaptcha/api",
  "schemes": ["https"],
  "paths": {
    "/siteverify": {
      "post": {
        "parameters": [
          {
            "name": "secret",
            "in": "formData",
            "required": true,
            "type": "string"
          },
          {
            "name": "response",
            "in": "formData",
            "required": true,
            "type": "string"
          },
          {
            "name": "remoteip",
            "in": "formData",
            "required": false,
            "type": "string"
          }
        ],
        "responses": {
          "200": {
            "description": "Captcha verification response"
          }
        }
      }
    }
  }
}
